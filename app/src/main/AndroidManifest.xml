<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nguyennam.financialbook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/presentation"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".accounttab.AccountMain" />
        <activity android:name=".budgettab.BudgetMain" />
        <activity android:name=".recordtab.RecordMain" />
        <activity android:name=".reporttab.ReportMain" />
        <activity android:name=".settingtab.SettingMain" />
        <activity android:name=".recordtab.Calculator"/>
        <activity android:name=".recordtab.ChildActivity"/>
    </application>

</manifest>